{
  "name": "react-static-page-webpack-plugin",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/4Catalyzer/react-static-page-webpack-plugin.git"
  },
  "author": "4Catalyzer",
  "license": "MIT",
  "scripts": {
    "eslint": "eslint . ",
    "format": "yarn eslint && yarn import-sort --write && yarn prettier --write",
    "import-sort": "import-sort '**/src/**/*.js' '!**/__generated__/*'",
    "lint": "yarn eslint && yarn import-sort --list-different && yarn prettier --list-different",
    "prettier": "prettier --ignore-path .eslintignore '**/*.{json,css,md}'",
    "release": "rollout",
    "tdd": "jest --watch",
    "test": "yarn lint && jest",
    "testonly": "jest"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": {
    "printWidth": 79,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "importSort": {
    ".js": {
      "parser": "@4c/import-sort/parser",
      "style": "@4c/import-sort/style"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "import-sort --write",
      "git add"
    ],
    "*.{json,css,md}": [
      "prettier --write --ignore-path .eslintignore",
      "git add"
    ]
  },
  "jest": {
    "preset": "@4c/jest-preset",
    "testEnvironment": "node"
  },
  "release": {},
  "devDependencies": {
    "@4c/import-sort": "^4.3.0",
    "@4c/jest-preset": "^1.3.0",
    "@4c/rollout": "^1.2.0",
    "eslint": "^5.12.0",
    "eslint-config-4catalyzer": "^0.7.2",
    "eslint-config-4catalyzer-jest": "^1.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.1.2",
    "import-sort-cli": "^5.2.0",
    "jest": "^24.0.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  },
  "dependencies": {
    "eval": "^0.1.2",
    "webpack-sources": "^1.3.0"
  },
  "peerDependencies": {
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  }
}
